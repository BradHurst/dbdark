{
  "name": "DBDark",
  "version": "0.2.5",
  "manifest_version": 2,
  "description": "Dark theme for Databricks",
  "icons": {
    "48": "img/logo.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.databricks.com/*", "https://*.azuredatabricks.net/*"],
      "css": [
        "css/editor.css",
        "css/markdown.css",
        "css/cell.css",
        "css/navbar.css",
        "css/widget.css",
        "css/body.css",
        "css/sidebar.css"
      ],
      "run_at": "document_idle"
    }
  ]
}
