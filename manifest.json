{
  "name": "Databricks Notebook Enhancer",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Enhance the editor functionality of Databricks notebook",
  "icons": {
    "48": "logo.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.databricks.com/*"],
      "css": [
        "css/editor.css",
        "css/markdown.css",
        "css/cell.css",
        "css/navbar.css",
        "css/body.css"
      ],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": ["main.js"]
}
