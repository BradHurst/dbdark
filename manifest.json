{
  "name": "DBDark",
  "version": "0.2.2",
  "manifest_version": 2,
  "description": "Dark theme for Databricks notebooks",
  "icons": {
    "48": "logo.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.databricks.com/*",
        "https://*.azuredatabricks.net/*"
      ],
      "css": [
        "css/editor.css",
        "css/markdown.css",
        "css/cell.css",
        "css/navbar.css",
        "css/widget.css",
        "css/body.css"
      ],
      "run_at": "document_idle"
    }
  ]
}
